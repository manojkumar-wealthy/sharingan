# ===========================================
# Market Pulse Multi-Agent API Configuration
# ===========================================

# Vertex AI Configuration
VERTEX_AI_PROJECT_ID=your-gcp-project-id
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_FAST_MODEL=gemini-2.0-flash-exp
VERTEX_AI_PRO_MODEL=gemini-1.5-pro-002

# Agent Timeouts (seconds)
MARKET_DATA_AGENT_TIMEOUT=10
NEWS_ANALYSIS_AGENT_TIMEOUT=30
USER_CONTEXT_AGENT_TIMEOUT=15
IMPACT_ANALYSIS_AGENT_TIMEOUT=45
SUMMARY_AGENT_TIMEOUT=20
ORCHESTRATOR_TIMEOUT=120

# API Configuration
API_PORT=8000
API_HOST=0.0.0.0
LOG_LEVEL=INFO
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Feature Flags
ENABLE_CACHING=true
ENABLE_TRACING=true
CACHE_TTL_SECONDS=300

# Redis (for caching)
REDIS_URL=redis://localhost:6379

# Data Source Placeholders (replace with actual APIs)
MARKET_DATA_API_URL=https://placeholder.api/market
NEWS_API_URL=https://placeholder.api/news
USER_DATA_SERVICE_URL=https://placeholder.api/users

# Google Cloud Authentication
# Set this to the path of your service account key file
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
